{
    "coreInit": "--federates=1",
    "coreName": "Griddlabd_sw Federate",
    "coreType": "zmq",
    "name": "gld_sw",
    "offset": "0.0",
    "period": 60,
    "timeDelta": "1.0",
    "logfile": "output2.log",
    "log_level": "warning",
    "publications" : [
          {
               "global" : true,
               "key": "load_meter/current",
               "type": "complex",
               "info" : {
                    "object" : "load_meter",
                    "property" : "measured_current_A"
               }
          }
     ],
     "subscriptions" : [
          {
               "required": true,
               "global" : true,
               "key": "Switch_Controller/sw1",
               "type": "string",
               "info" : {
                    "object" : "load_switch",
                    "property" : "status"
                    }
          }
     ]
}
